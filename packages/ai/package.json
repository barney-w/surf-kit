{
  "name": "@surf-kit/ai",
  "version": "0.1.0",
  "description": "Adapter bridging surf-kit agent components with Vercel AI SDK v6",
  "keywords": [
    "react",
    "ai",
    "vercel-ai-sdk",
    "adapter",
    "chat",
    "streaming",
    "design-system"
  ],
  "license": "0BSD",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/barney-w/surf-kit.git",
    "directory": "packages/ai"
  },
  "homepage": "https://github.com/barney-w/surf-kit#readme",
  "bugs": {
    "url": "https://github.com/barney-w/surf-kit/issues"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@surf-kit/agent": "workspace:*",
    "ai": "^6.0.0",
    "@ai-sdk/react": "^3.0.0"
  },
  "devDependencies": {
    "@surf-kit/tsconfig": "workspace:*",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "jsdom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  },
  "publishConfig": {
    "access": "public"
  }
}

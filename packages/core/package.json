{
  "name": "@surf-kit/core",
  "version": "0.1.1",
  "description": "Accessible React UI primitives â€” Button, Input, Card, Table, Dialog, and more",
  "keywords": [
    "react",
    "components",
    "ui",
    "design-system",
    "accessible",
    "react-aria",
    "tailwind"
  ],
  "license": "0BSD",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/barney-w/surf-kit.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/barney-w/surf-kit#readme",
  "bugs": {
    "url": "https://github.com/barney-w/surf-kit/issues"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./primitives": {
      "types": "./dist/primitives/index.d.ts",
      "import": "./dist/primitives/index.js",
      "require": "./dist/primitives/index.cjs"
    },
    "./inputs": {
      "types": "./dist/inputs/index.d.ts",
      "import": "./dist/inputs/index.js",
      "require": "./dist/inputs/index.cjs"
    },
    "./feedback": {
      "types": "./dist/feedback/index.d.ts",
      "import": "./dist/feedback/index.js",
      "require": "./dist/feedback/index.cjs"
    },
    "./overlay": {
      "types": "./dist/overlay/index.d.ts",
      "import": "./dist/overlay/index.js",
      "require": "./dist/overlay/index.cjs"
    },
    "./navigation": {
      "types": "./dist/navigation/index.d.ts",
      "import": "./dist/navigation/index.js",
      "require": "./dist/navigation/index.cjs"
    },
    "./data": {
      "types": "./dist/data/index.d.ts",
      "import": "./dist/data/index.js",
      "require": "./dist/data/index.cjs"
    },
    "./layout": {
      "types": "./dist/layout/index.d.ts",
      "import": "./dist/layout/index.js",
      "require": "./dist/layout/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@surf-kit/hooks": "workspace:*",
    "@surf-kit/theme": "workspace:*",
    "@surf-kit/tokens": "workspace:*",
    "class-variance-authority": "catalog:",
    "react-aria": "catalog:",
    "@internationalized/date": "^3.11.0",
    "react-stately": "catalog:",
    "tailwind-merge": "catalog:"
  },
  "devDependencies": {
    "@surf-kit/test-utils": "workspace:*",
    "@surf-kit/tsconfig": "workspace:*",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/user-event": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "jsdom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "vitest-axe": "catalog:"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  },
  "publishConfig": {
    "access": "public"
  }
}

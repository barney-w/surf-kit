{
  "duration": {
    "$type": "duration",
    "instant": { "$value": "100ms" },
    "fast": { "$value": "200ms" },
    "normal": { "$value": "300ms" },
    "slow": { "$value": "500ms" },
    "streaming": { "$value": "50ms" }
  },
  "easing": {
    "$type": "cubicBezier",
    "default": { "$value": "cubic-bezier(0.4, 0, 0.2, 1)" },
    "in": { "$value": "cubic-bezier(0.4, 0, 1, 1)" },
    "out": { "$value": "cubic-bezier(0, 0, 0.2, 1)" },
    "in-out": { "$value": "cubic-bezier(0.4, 0, 0.2, 1)" }
  }
}
